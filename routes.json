{
  "item_index": {
    "path": "/api/v1/items",
    "method": "GET",
    "controller": "ItemController",
    "action": "index",
    "middlewares": [
      "auth"
    ]
  },
  "item_show": {
    "path": "/api/v1/items/([0-9]*)",
    "method": "GET",
    "controller": "ItemController",
    "action": "show",
    "middlewares": [
      "auth"
    ]
  },
  "register": {
    "path": "/api/v1/register",
    "method": "POST",
    "controller": "AuthController",
    "action": "register"
  }
}

